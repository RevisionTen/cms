{% if value %}
    {% if value.deleted %}<span class="text-danger" title="{{ 'admin.label.deleted'|trans({}, 'cms') }}"><span class="fas fa-trash"></span> {% endif %}
    {% if value.deleted == false %}<a href="{{ path('cms_edit_aggregate', {id: value.id}) }}"><span title="{{ value.published ? ('admin.label.published'|trans({}, 'cms')) : ('admin.label.unpublished'|trans({}, 'cms')) }}"><span class="fas fa-circle {{ value.published ? 'text-success' : 'text-danger'}}"></span> {% endif %}
        {{ value.title }}
    {% if value.deleted == false %}</span></a>{% endif %}
    {% if value.deleted %}</span>{% endif %}
{% else %}
    {{ 'Controller or redirect only'|trans }}
{% endif %}
