{% extends '@EasyAdmin/default/layout.html.twig' %}

{% block content_title %}
    {% if title is defined %}
        {{ title|trans({}, 'cms') }}
    {% endif %}
{% endblock %}

{% block main %}
    {{ form_start(form) }}
    {{ form_errors(form) }}
    {% if form.data is defined %}
        {{ form_widget(form.data) }}
    {% endif %}
    {{ form_end(form) }}
{% endblock %}
