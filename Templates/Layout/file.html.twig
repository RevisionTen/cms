<div {{ editorAttr(element, edit, 'bg-warning') }}>
    {% if element.data.managedFile.file.uuid is defined and element.data.managedFile.file.path is defined %}
        <a class="btn btn-primary" href="{{ element.data.managedFile.file.path }}">{{ element.data.managedFile.title }}</a>
    {% endif %}
</div>
